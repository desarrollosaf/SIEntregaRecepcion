generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  
}

model roles {
  id   Int    @id @default(autoincrement())
  role String
}

model registro {
  id               Int      @id @default(autoincrement())
  rfc_entrega      String
  rfc_recibe       String
  id_departamento  Int
  fecha_movimiento DateTime
  rfc_movimiento   String
  status           Boolean  @default(true)
}

model documentos {
  id Int @id @default(autoincrement())
  id_departamento Int
  path_marco String?
  path_manual String?
  path_doc1 String?
  path_doc2 String?
  status Boolean @default(true)
}
