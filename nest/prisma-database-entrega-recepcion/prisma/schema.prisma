generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  
}

model roles {
  id   Int    @id @default(autoincrement())
  role String
}

model registro {
  id               Int      @id @default(autoincrement())
  rfc_entrega      String
  rfc_recibe       String
  id_departamento  Int
  fecha_movimiento DateTime
  rfc_movimiento   String
  status           Boolean  @default(true)
}
